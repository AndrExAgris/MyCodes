n = int(input())

#fatora em valores primos e os coloca em um vetor
divisores = []
p = 2
while n > 1: 
	if n%p == 0:
		n = n/p
		divisores.append(p)
	else:
		p += 1
    
#separa os valores primos e quantas vezes se repetem em vetores diferentes
a = -1
expoentes = []
fatoresprimos = []
for i in divisores:
    if i in fatoresprimos:
        expoentes[a]+=1
    else:
        expoentes.append(0)
        a+=1
        expoentes[a]+=1
        fatoresprimos.append(i)

#calculo que da a soma de todos os diivisores com base na fatoração em primos
s=1
for k in range(len(fatoresprimos)):
    a = fatoresprimos[k]
    b = expoentes[k]
    s *=((a**(b+1))-1)/(a-1)

print(int(s))
